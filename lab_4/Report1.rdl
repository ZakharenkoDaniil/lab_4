  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT districts.Название, COUNT(Otproviteliy1.Код_отправителя) AS Expr1
FROM     districts INNER JOIN
                  Otproviteliy1 ON districts.адрес = Otproviteliy1.Адрес
GROUP BY districts.Название
ORDER BY Expr1 DESC</CommandText>
      </Query>
